{
    "name": "adminui/adminui-installer",
    "description": "My awesome package",
    "keywords": [
        "adminui",
        "adminui-installer"
    ],
    "homepage": "https://github.com/adminui/adminui-installer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "DSM Design Ltd",
            "email": "c.riley@dsmdesign.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AdminUI\\AdminUIInstaller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminUI\\AdminUIInstaller\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AdminUI\\AdminUIInstaller\\AdminUIInstallerServiceProvider"
            ],
            "aliases": {
                "AdminUIInstaller": "AdminUI\\AdminUIInstaller\\AdminUIInstallerFacade"
            }
        }
    }
}
